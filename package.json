{
  "name": "provenance-frontend-explorer",
  "homepage": "/explorer",
  "description": "Blockchain explorer for the Provenance Blockchain",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/provenance-io/explorer-frontend"
  },
  "version": "1.2.2",
  "private": false,
  "sideEffects": false,
  "dependencies": {
    "@provenanceio/wallet-lib": "0.5.0",
    "axios": "0.21.1",
    "bech32": "2.0.0",
    "big.js": "6.1.1",
    "date-fns": "2.16.1",
    "date-fns-tz": "1.0.12",
    "echarts": "4.9.0",
    "format-number": "3.0.0",
    "react": "16.13.1",
    "react-datepicker": "3.3.0",
    "react-dom": "16.13.1",
    "react-json-view": "1.21.1",
    "react-number-format": "4.4.1",
    "react-redux": "7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-tiny-hooks": "1.0.2",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-thunk": "2.3.0",
    "styled-components": "5.0.1",
    "styled-normalize": "8.0.7",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.1",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "react-scripts": "4.0.3",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "4.1.0",
    "source-map-explorer": "2.5.0"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "npm run start:test",
    "local": "npm run start:local",
    "start:test": "react-scripts start",
    "start:local": "REACT_APP_ENV=local react-scripts start",
    "start:prod": "REACT_APP_ENV=production react-scripts start",
    "prebuild": "npm run lint && npm test -- --watchAll=false",
    "build": "react-scripts build",
    "build:stats": "react-scripts build -- --stats",
    "linkify": "python ./scripts/linkify.py ./CHANGELOG.md",
    "test": "react-scripts test",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "stats": "npm run build:stats && webpack-bundle-analyzer --port 4200 build/bundle-stats.json",
    "analyze": "npm run build && source-map-explorer build/static/js/*.js --only-mapped",
    "docker:local": "docker build -t explorer-frontend:dev .",
    "docker:prod": "docker build -f Dockerfile-prod -t explorer-frontend:prod ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
